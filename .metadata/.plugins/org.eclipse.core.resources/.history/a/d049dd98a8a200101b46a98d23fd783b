
public class Book {

	
		String isbn;
		String Author;
		int  version;
		
		Book () //no-arg constructor
		{
			
			isbn="548245hfghf";
			Author="Micheal Johns";
			version=1;
		}
		
		Book (String newisbn, String newAuthor) //non-default constructor
		{
			isbn=newisbn;
			Author=newAuthor;
		}
		
		Book (String newisbn, String newAuthor, int newVersion) //overloaded non-default constructor
		{
			isbn=newisbn;
			Author=newAuthor;
			version=newVersion;
		}
		
		void display(){
			System.out.println("Book ISBN: "+isbn);
			System.out.println("Book Author"+Author);
			System.out.println("Book Version"+version);
		}
		
}
